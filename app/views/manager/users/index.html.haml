#content
  %h1 Welcome #{@current_user.name}!
  %p from #{@current_user.company.name}

  = button_to 'New Employee', new_manager_users_path
  -if @employee.present?
    -@employees.each do |employee|
      %div.block
        %ul
          %h3= employee.name
          %li= employee.email
          %li #{employee.address}, #{employee.city}
          %li= link_to 'edit info', edit_manager_users_path(employee.id)

