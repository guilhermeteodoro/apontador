- presenter = UserPresenter.new(manager: @current_user, employee: @employee)

%h1 Editar #{presenter.edit_employee_title}
%h2= @employee.name

#message= flash_message

.base
  = form_for @employee, url: eval(presenter.edit_employee_form_url) do |f|
    = render partial: 'form', locals: { f: f, edit: true }

